    "def flist(args):\n",
    "    print('参数个数:',len(args))\n",
    "    for i,k in enumerate(args):\n",
    "        print(\"第{}个参数{}:\".format(i,k))\n",
    "    print(args)\n",
    "b=[]\n",
    "a = input('>>>:').split()\n",
    "for j in a:\n",
    "    b.append(int(j))\n",
    "flist(b)"
